{
  "name": "env-switcher",
  "displayName": ".ENV Switcher",
  "description": "Switch .env file content using presets, straight from the status bar",
  "icon": "images/env-switcher.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/EcksDy/vscode-env-switcher.git"
  },
  "publisher": "EcksDy",
  "version": "0.1.3",
  "engines": {
    "vscode": "^1.37.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "workspaceContains:**/.env"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.selectEnv",
        "title": ".ENV Switcher: Select .env file"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/node": "^14.0.5",
    "@types/vscode": "1.37.0",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.34.0",
    "eslint": "^7.0.0",
    "eslint-config-airbnb-typescript": "^7.2.1",
    "eslint-config-airbnb-typescript-prettier": "^2.1.1",
    "eslint-plugin-import": "^2.20.1",
    "typescript": "^3.9.3",
    "prettier": "^2.0.5"
  },
  "peerDependencies": {
    "glob": "*"
  },
  "dependencies": {
    "glob": "^7.1.6",
    "glob-promise": "^3.4.0"
  }
}
